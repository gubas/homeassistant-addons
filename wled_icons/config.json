{
  "name": "WLED Icons",
  "version": "1.0.5",
  "slug": "wled_icons",
  "description": "Affichage d'icônes LaMetric animées sur matrice WLED 8x8",
  "url": "https://github.com/gubas/homeassistant-addons",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "armhf",
    "i386"
  ],
  "startup": "application",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 8234,
  "hassio_api": true,
  "panel_icon": "mdi:led-strip-variant",
  "ports": {
    "8234/tcp": 8234
  },
  "homeassistant": "2024.6.0",
  "map": [
    "config:rw",
    "share:rw"
  ],
  "options": {
    "log_level": "INFO"
  },
  "schema": {
    "log_level": "list(DEBUG|INFO|WARNING|ERROR)"
  }
}